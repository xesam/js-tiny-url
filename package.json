{
  "name": "@js-tiny/url",
  "version": "0.0.1",
  "description": "A simple url-style-string builder with pure javascript",
  "keywords": [
    "url",
    "parse url",
    "build url"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "directories": {
    "src": "src",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "license": "MIT",
  "dependencies": {
    "@xesam/url": "~0.1.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "author": "xesam <xesam@outlook.com>",
  "homepage": "https://github.com/xesam/js-pure-url#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xesam/js-pure-url.git"
  },
  "bugs": {
    "url": "https://github.com/xesam/js-pure-url/issues"
  }
}
